<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Twitter</th>
      <th>User Id </th>
      <th> </th>
    </tr>
  </thead>

  <tbody id="friends">
    <% @friends.each do |friend| %>
    <tr></tr>
      <%= render friend %>
      
    <% end %>
  </tbody>

  <br>

  <%= link_to 'New Friend', new_friend_path, class:"btn btn-secondary"%>

</table>